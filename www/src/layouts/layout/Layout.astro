---
import {ApplicationLayout, IndexLayout, PageLayout} from "@/layouts/index";


export type LayoutType = "default" | "application" | "blog" | "page";

export interface Props { 
    layout: LayoutType;
}

export const Layouts = {
  "default": IndexLayout,
  "application": ApplicationLayout,
  "blog": IndexLayout, 
  "page": PageLayout
};

const { layout = "default" } = Astro.props;

---


<IndexLayout>
    <slot />
</IndexLayout>