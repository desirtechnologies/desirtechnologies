---
import ApplicationLayout from "@/layouts/application";
import Footer from "@/includes/Footer/index.ts"
import  Header  from "@/includes/Header/"
import { Navigator } from "@/includes/Navigator/index.ts"

import Head from "@/includes/Head"

interface Props {
  title?: string;
  description?: string | null;
  image?: string;
  noindex?: boolean;
  canonical?: string;
}

const { title, description, image, noindex, canonical } = Astro.props;
---

<ApplicationLayout>
    <Header slot="header" />
    <Navigator slot="navigator" />
    <slot slot="content"/>
    <Footer slot="footer" />
</ApplicationLayout>
